}

.model .item-box .item:before {
  content: "";
  position: absolute;
  left: 30rpx;
  right: 0;
  bottom: 0;
  height: 2rpx;
  background: #ebebe7;
}

.model .item {
  display: flex;
}

.model .item .name {
