.catalog {
  position: relative;
  flex: 1;
  width: 100%;
  background: #fff;
  display: flex;
  margin-bottom: 100rpx;
}

.catalog .cate {
  width: 100%;
  height: 100%;
  flex: 1;
  position: absolute;
  top:0;
  bottom: 0;
  right: 0;
  z-index: 5;
}

.catalog .bd {
  height: auto;
  width: 100%;
  overflow: hidden;
  display: flex;
  flex-wrap: wrap
}

.catalog .bd .item {
  display: block;
  width: 50%;
  height: 460rpx;
  padding:  20rpx  30rpx  20rpx  30rpx;
  position: relative;
  border: 1rpx solid #e7e7eb;
}

 .catalog .bd .item .icon {
  width: 100%;
  height: 260rpx;
} 

.catalog .bd .item .txt {
  display: -webkit-box;
  font-size: 26rpx;
  color: #333;
  flex-wrap: wrap;
  text-overflow:ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.catalog .bd .item .price {
  position: absolute;
  left: 30rpx;
  right: 0;
  bottom: 20rpx;
  color: red;
}

.catalog .bd .item .price text {
  color: inherit;
  font-size: 30rpx;
}

.catalog .bd .crol {
  text-align: right;
  padding-right: 30rpx;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 10rpx;
}

.catalog .bd .remove,  .catalog .bd .add, .model .remove, .model .add, .preview-model .add, .preview-model .remove {
    border:10rpx solid transparent;
    width: 60rpx;
    display: inline-block;
    height: 60rpx;
    padding:0;
    border-radius: 50%;
    text-align: center;
    color: #fff;
    position: relative;
    vertical-align: middle;
    transform: translateY(-2rpx);
    background-clip: padding-box!important
}

.catalog .bd .remove:before, .catalog .bd .add:before, .catalog .bd .add:after, .model .remove:before, .model .add:before, .model .add:after , .preview-model .remove:before, .preview-model .add:before, .preview-model .add:after{
  content: "";
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  width: 20rpx;
  height: 4rpx;
  background: #fff;
}
.catalog .bd .add:after, .model .add:after, .preview-model .add:after {
  width: 4rpx;
  height: 20rpx;
}

.catalog .bd .num, .model .num, .preview-model .num {
    padding: 0 10rpx;
    min-width: 40rpx;
    text-align: center;
    display: inline-block;
    height: 40rpx;
    line-height: 40rpx;
}

.foot {
  height: 100rpx;
  display: flex;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: white;
  border-top: 1rpx solid #EAEAEA;
  border-bottom: 1rpx solid #EAEAEA;
}

.foot .left {
  width: 78%;
  display: flex;
}

.foot .left .img_wrap {
  width: 120rpx;
  position: relative;
  background: white;
  color: #fff;
}

.foot .left .img_wrap image {
  width: 70rpx;
  height: 70rpx;
  position: absolute;
  left: 20rpx;
  top: 15rpx;
}

.img_wrap text {
  position: absolute;
  right: 20rpx;
  top: 15rpx;
  min-width: 30rpx;
  height: 30rpx;
  padding: 0 5rpx;
  background: red;
  border-radius: 15rpx;
  color: #fff;
  line-height: 30rpx;
  text-align: center;
}

.foot .left .checked_list {
  background: white;
  color: #fff;
  flex: 1
}

.foot .submit {
  flex: 1;
  text-align: center;
  line-height: 100rpx;
  background: white;
  color: #fff;
  font-size: 32rpx;
  /*margin: 15rpx;*/
  /*border-radius: 12rpx*/
}

.foot .left .noData {
  line-height: 100rpx;
  color: #333;
}

.foot .left .price {
  line-height: 100rpx;
  color: #434343;
}

.foot .left .price text {
  font-size: 35rpx;
  color: inherit;
  font-weight: 700;
}

.model {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 100rpx;
  z-index: 10;
}

.model .bg {
  background: rgba(0,0,0,0.4);
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  opacity: 0
}

.model .list {
  background: #fff;
  min-height: 100rpx;
  position: absolute;
  bottom: -100%;
  left: 0;
  right: 0;
  z-index: 10
}

.model .ctrl {
  line-height: 80rpx;
  display: flex;
  justify-content: space-between;
  background: white;
  border-bottom: 1rpx solid #EAEAEA;
}

.model .ctrl .left {
