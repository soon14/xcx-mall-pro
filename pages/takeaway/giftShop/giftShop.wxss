  font-size: 25rpx;
}

.model .ctrl .right .takeoutclearimage{
  position: relative;
  top: 10rpx;
  bottom:0;
  left: 0;
  width: 40rpx;
  height: 40rpx;
}

.model .item-box {
  min-height: 50rpx;
  max-height: 600rpx;
  overflow: auto;
}

.model .item-box .item {
  line-height: 100rpx;
  padding: 10rpx 30rpx;
