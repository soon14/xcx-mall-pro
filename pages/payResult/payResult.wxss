page {
  min-height: 100%;
  width: 100%;
  background: #fff;
}

.container {
  height: 100%;
  background: #fff;
}

.pay-result {
  background: #fff;
}

.icon-status {
  align-items: center;
  justify-content: center;
  margin-top: 64rpx;
  margin-bottom: 32rpx;
  height:120rpx;
  width:120rpx;
  margin-left:calc(50% - 60rpx);
}

.pay-result .msg {
  text-align: center;
  font-weight:400;
  color:rgba(67,67,67,1);
  line-height:36rpx;
  font-size: 28rpx;
  height:36rpx;
}

.pay-result .money {
  text-align: center;
  font-weight:400;
  color:#434343;
  line-height:48rpx;
  font-size: 40rpx;
  height:48rpx;
  margin-top:16rpx;
}

.pay-result .btns {
  margin-top:32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.pay-result .btn:nth-child(1) {
  text-align: center;
  height: 80rpx;
  margin: 0 20rpx;
  width: 272rpx;
  line-height: 78rpx;
  border: 1rpx solid #999999;
  color: #999999;
  border-radius: 8rpx;
  font-size:28rpx;
}

.pay-result .btn:nth-child(2) {
  text-align: center;
  height: 80rpx;
  margin: 0 20rpx;
  width: 272rpx;
  line-height: 78rpx;
  background:rgba(67,67,67,1);
  color: white;
  border-radius:8rpx;
  font-size:28rpx;
}

.pay-result .error .msg {
  color: #b4282d;
  margin-bottom: 60rpx;
}

.pay-result .error .tips {
  color: #7f7f7f;
  margin-bottom: 70rpx;
}

.pay-result .error .tips .p {
  font-size: 24rpx;
  line-height: 42rpx;
  text-align: center;
}

.pay-result .error .tips .p {
  line-height: 42rpx;
  text-align: center;
}



.coupon{
  margin-top: 128rpx;
  display: flex;
  flex-direction: column;
  background:rgba(249,249,249,1);
  margin-left: 30rpx;
  margin-right: 30rpx; 
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  margin-bottom: 60rpx;
}

.coupon_tips{
  font-size: 24rpx;
  color: rgba(226,174,104,1);
  line-height:64rpx;
  height: 64rpx;
  text-align: center
}

.coupon_list{
  display: flex;
  flex-wrap:  wrap;
  padding-left: 30rpx;
  justify-content: start;
}
.coupon_item{
  width: 200rpx;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: white;
  margin-right: 20rpx;
  margin-top: 20rpx;
}
.coupon_item_monery{
  display: flex;
}
.coupon_item_monery_symbol{
  color: red;
  font-size: 30rpx;
  align-self: flex-end;
  padding-bottom: 10rpx;
}
.coupon_item_monery_monery{
  color: red;
  font-size: 50rpx;
  align-self: flex-end
}
.coupon_item_condition{
    color: rgba(102,102,102,1);
    font-size:24rpx;
}
.coupon_item_tips{
  color:rgba(255,154,29,1);
  font-size:20rpx;
  border: rgba(255,154,29,1) solid 1rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
}
.coupon_share{
    border:1px solid rgba(226,174,104,1);
    border-radius:32px;
    width: 180rpx;
    align-self: center;
    color: rgba(226,174,104,1);
    margin-top: 40rpx;
    font-size: 24rpx;
    background: white;
}