.catalog .bd .remove,  .catalog .bd .add, .model .remove, .model .add, .preview-model .add, .preview-model .remove {
    border:10rpx solid transparent;
    width: 60rpx;
    display: inline-block;
    height: 60rpx;
    border-radius: 50%;
    text-align: center;
    color: #fff;
    position: relative;
    vertical-align: middle;
    transform: translateY(-2rpx);
    background-clip: padding-box!important
}

.catalog .bd .remove:before, .catalog .bd .add:before, .catalog .bd .add:after, .model .remove:before, .model .add:before, .model .add:after , .preview-model .remove:before, .preview-model .add:before, .preview-model .add:after{
  content: "";
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  width: 20rpx;
  height: 4rpx;
  background: #fff;
}
.catalog .bd .add:after, .model .add:after, .preview-model .add:after {
  width: 4rpx;
  height: 20rpx;
}

.catalog .bd .num, .model .num, .preview-model .num {
    padding: 0 10rpx;
    min-width: 40rpx;
    text-align: center;
    display: inline-block;
    height: 40rpx;
    line-height: 40rpx;
}

