.weui-form-preview{
  width:750rpx;
}
.weui-form-preview__bd {
  width: 750rpx;
  height: auto;
  background:#FFFFFF;
  padding:0;
  margin-bottom: 32rpx;
}
.weui-form-preview__item{
  width:750rpx;
  height:100rpx;
  display: flex;
  align-items:center;
  margin-left:32rpx;
  border-bottom: 0.5rpx solid rgb(245, 242, 242);

}
.weui-form-preview__item .right{
  flex:1;
  margin-right: 64rpx;
}
.weui-form-preview__item image{
  width:30rpx;
  height: 30rpx;
  
}
.weui-form-preview__item .qb-icon{
  width:30rpx;
  height: 30rpx;
  margin-left:160rpx;
  margin-top:-45rpx;
}
.weui-form-preview__label  {
  color: #333333;
  font-size:28rpx;
}

.weui-form-preview__value {
  color: #999999;
  font-size: 24rpx;
  text-align: left;
  width:384rpx;
}
.weui-form-preview__item-time{
  width:750rpx;
  height:auto;
  margin-left:32rpx;
  border-bottom: 0.5rpx solid rgb(245, 242, 242);
}
.weui-form-preview__item-time .weui-form-preview__value{
  width:500rpx;
}
.weui-form-preview__item-time .week-time{
  display: flex;
  justify-content: flex-start;
  align-content: center;
}
.weui-form-preview__item-time .week-time view{
  font-size: 24rpx;
  color:#999999;
}
.weui-form-preview__item-time .buss-time{
  font-size: 24rpx;
  color:#999999;
}
.weui-form-preview__item1{
  width:750rpx;
  height:264rpx;
  border-bottom: 0.5rpx solid rgb(245, 242, 242);
}
.weui-form-preview__item1 .list{
  width:100%;
  height:148rpx;
  white-space:nowrap;
  padding:0;
}
.weui-form-preview__item1 .weui-form-preview__label{
  height:36rpx;
  margin-top:32rpx;
  margin-bottom: 12rpx;
  margin-left:32rpx;
}
.weui-form-preview__picture {
  clear:both;
  width:750rpx;
  height:148rpx;
}
.weui-form-preview__picture image {
  width:224rpx;
  height:148rpx;
  margin-right:12rpx;
}
.weui-form-preview__picture image:nth-child(1){
  float: left;
  margin-left:32rpx;
}
.weui-form-preview__picture image:last-child{
  margin-right: 32rpx;
}
.weui-form-preview__item-desc{
  width: 750rpx;
  height:180rpx;
  border-bottom: 0.5rpx solid rgb(245, 242, 242);
  margin-left:32rpx;
}
.weui-form-preview__item-desc:last-child{
  border:none;
}
.weui-form-preview__item-desc .weui-form-preview__label{
  height:36rpx;
  margin-top:32rpx;
  margin-bottom: 12rpx;
  /* margin-left:32rpx; */
}
.weui-form-preview__description{
  clear:both;
  width:672rpx;
  height:64rpx;
  line-height:32rpx;
  margin:24rpx 48rpx 32rpx 0rpx;

  font-size: 24rpx;
  color:#999999;
  text-align:left;
}
